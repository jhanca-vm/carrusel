{
  "name": "carrusel",
  "version": "1.2.2",
  "description": "A simple and dependency-free carousel for Svelte that is SSR-compatible.",
  "keywords": [
    "carousel",
    "slider",
    "svelte",
    "sveltekit"
  ],
  "author": "Jhan Viloria",
  "license": "MIT",
  "homepage": "https://carruseljs.vercel.app",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jhanca-vm/carrusel"
  },
  "bugs": {
    "url": "https://github.com/jhanca-vm/carrusel/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "svelte": "^4.0.0"
  },
  "dependencies": {
    "@fontsource-variable/dosis": "^5.0.4"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^2.0.2",
    "@sveltejs/kit": "^1.22.0",
    "@sveltejs/package": "^2.1.0",
    "eslint": "^8.44.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-svelte": "^2.32.2",
    "mdsvex": "^0.11.0",
    "publint": "^0.1.16",
    "svelte": "^4.0.4",
    "svelte-check": "^3.4.5",
    "tslib": "^2.6.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.1"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module"
}
