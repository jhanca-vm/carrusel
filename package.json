{
  "name": "carrusel",
  "version": "0.1.1-beta.1",
  "description": "A simple and dependency-free carousel for Svelte that is SSR-compatible.",
  "keywords": [
    "carousel",
    "slider",
    "svelte"
  ],
  "author": "Jhan Viloria",
  "license": "MIT",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jhanca-vm/carrusel"
  },
  "bugs": {
    "url": "https://github.com/jhanca-vm/carrusel/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "svelte": "^3.54.0"
  },
  "dependencies": {
    "@fontsource-variable/dosis": "^5.0.3"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^2.0.2",
    "@sveltejs/kit": "^1.5.0",
    "@sveltejs/package": "^2.0.0",
    "publint": "^0.1.9",
    "svelte": "^3.54.0",
    "svelte-check": "^3.0.1",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^4.3.0"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module"
}
